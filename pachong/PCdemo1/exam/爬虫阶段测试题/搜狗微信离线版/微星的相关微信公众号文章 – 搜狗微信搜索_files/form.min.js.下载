(function(){var a=$("input[name=query]"),b=$("[name=reset]");a.keyup(function(){""!=$(this).val()?b.show():b.hide()});b.click(function(){$(this).siblings("input.query").focus();a.val("");a.attr("value","");b.hide()});if(a&&2==a.length){var d=a.eq(0),c=a.eq(1);c.attr("id","query_bottom");a.on("input",function(){d.is(":focus")&&c.val(d.val());c.is(":focus")&&d.val(c.val())})}})();